.container {
  background-color: white;
  font-size: 45px;
}

image {
  width: 48px;
  height: 48px;
}

.item {
  width: 100%;
  height: 30px;
}

.logo {
  height: 70px;
}

.item  view {
  height: 100%;
  margin-left: 10px;
  float: left;
  -webkit-display: flex;
  display: flex;
  -webkit-align-items: center;
  align-items: center;
  -webkit-justify-content: center;
  justify-content: center;
}

.item .arrow {
  float: right;
  margin-right: 20px;
}

.slide-image {
  width: 100%;
  height: 100px;
}


.cate-item{
    margin-top: 94rpx;
    height: auto;
    overflow: hidden;
}

 .h{
    height: 145rpx;
    width: 750rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

 .h .name{
    display: block;
    height: 35rpx;
    margin-bottom: 18rpx;
    font-size: 30rpx;
    color: #333;
}

.h .desc{
    display: block;
    height: 24rpx;
    font-size: 24rpx;
    color: #999;
}

 .b{
  width: 750rpx;
  padding: 0 6.25rpx;
  height: auto;
  overflow: hidden;
}

.b .item{
  float: left;
  background: #fff;
  width: 365rpx;
  margin-bottom: 6.25rpx;
  padding-bottom: 33.333rpx;
  height: auto;
  overflow: hidden;
  text-align: center;
}

 .b .item-b{
  margin-left: 6.25rpx;
}

 .item .img{
  width: 302rpx;
  height: 302rpx;
}

 .item .name{
  display: block;
  width: 365.625rpx;
  height: 35rpx;
  margin: 11.5rpx 0 22rpx 0;
  text-align: center;
  overflow: hidden;
  padding: 0 20rpx;
  font-size: 30rpx;
  color: #333;
}

 .item .price{
  display: block;
  width: 365.625rpx;
  height: 30rpx;
  text-align: center;
  font-size: 30rpx;
  color: #b4282d;
}