/* components/category/category.wxss */
.category {
  display: -webkit-flex; /* Safari */
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: flex-start;
  align-items: center;
  width: 100%;
}
.categoryItem {
  height: 80px;
}
.categoryItem span {
  display: block;
  width: 100%;
  text-align: center;
  font-weight: 600;
}
.categoryItem image{
  width: 187rpx;
  height: 60px;
}